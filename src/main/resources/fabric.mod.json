{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "Bawnorton"
  ],
  "contact": {
    "homepage": "https://github.com/Bawnorton",
    "sources": "https://github.com/Bawnorton/${mod_name}",
    "issues": "https://github.com/Bawnorton/${mod_name}/issues"
  },
  "license": "MIT",
  "icon": "assets/${mod_id}/icon.png",
  "environment": "*",
  "accessWidener": "${minecraft_version}.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.0",
    "minecraft": "${minecraft_dependency}",
    "configurable": ">=3.2.1",
    "fabric-api": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "${mod_id}.website": "https://github.com/Bawnorton/${mod_name}"
      }
    }
  }
}